<editors>
    <!-- Boolean -->
    <editor id="bool">
        <range uom="2" subset="0,1"/>
    </editor>
    <editor id="I_DURATION">
        <range uom="45" subset="0,1,5,20"/>
    </editor>
    <editor id="I_PERCENT">
        <range uom="51" min="1" max="100" prec="0" step="1" />
    </editor>
    <editor id="I_SWITCH_STATUS">
        <range uom="25" subset="0,1,2,3" nls="ISS"/>
    </editor>
    <editor id="I_DEBUG">
        <range uom="25" subset="9,10,20,30,40,50" nls="CDM"/>
    </editor>
    <editor id="LEVEL">
        <range uom="51" min="1" max="100" step="1" prec="0"/>
    </editor>
    <editor id="MODE">
        <range uom="25" subset="0-1" nls="MODE" />
    </editor>
    <editor id="MODECL">
        <range uom="25" subset="0-15" nls="MODECL" />
    </editor>
    <editor id="MODESW">
        <range uom="25" subset="0-1" nls="MODESW" />
    </editor>
    <editor id="LEVELPIR2">
        <range uom="56" min="0" max="100" step="1" prec="0"/>
    </editor>
    <editor id = "PIR2" > 
    <range  uom="56" min="0" max="10000" step="1" prec="0" />
    </editor>
    <editor id="PIR2C">
        <range uom="25" subset="0-5" nls="PIR2C" />
    </editor>
    <editor id="PIRLEVEL">
        <range uom="56" min="0" max="9" step="1" prec="0"/>
    </editor>
    <editor id="PIRDETEC">
        <range uom="38" min="0" max="1000" step="1" prec="0"/>
    </editor>
</editors>